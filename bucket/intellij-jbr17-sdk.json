    },
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "env_add_path": "bin",
    "checkver": {
        "url": "https://github.com/JetBrains/JetBrainsRuntime/releases",
        "regex": "\\s(?<Version>[\\d.]+)-(?<Build>[\\w]+)\\.(?<Patch>[\\d]+)</h2>",
        "replace": "${Version}-${Build}.${Patch}"
