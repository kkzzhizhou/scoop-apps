        "replace": "${dragon}-${jdk}${b}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/dragonwell-project/dragonwell21/releases/download/$matchTag/$matchFilename.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "regex": "$sha256"
