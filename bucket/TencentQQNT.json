            "if (Test-Path $S4UtilsPath) {",
            "    Unblock-File $S4UtilsPath",
            "    Import-Module $S4UtilsPath",
            "    Dismount-ExternalRuntimeData -Target \"QQ\" -AppData",
            "    Remove-Module -Name S4Utils -ErrorAction SilentlyContinue",
            "}"
