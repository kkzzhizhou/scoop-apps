    }
  },
  "shortcuts": [
    [
      "bin\\64bit\\obs64.exe",
      "OBS Studio"
    ]
  ],
  "pre_install": "if (!(Test-Path \"$persist_dir\\portable_mode.txt\")) { New-Item \"$dir\\portable_mode.txt\" | Out-Null }",
  "persist": [
    "config",
    "data",
    "obs-plugins",
    "portable_mode.txt"
  ],
  "checkver": {
    "url": "https://obsproject.com/download",
    "regex": "OBS-Studio-([\\d.]+)-Full-x64\\.zip"
  },
  "autoupdate": {
