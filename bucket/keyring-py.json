{
    "version": "25.3.0",
    "homepage": "https://github.com/jaraco/keyring",
    "description": "Provides an easy way to access the system keyring service from Python.",
    "license": "MIT",
    "depends": "python-bucket-utils",
    "url": "https://raw.githubusercontent.com/TheRandomLabs/Scoop-Python/master/placeholder.txt",
    "hash": "8f5572e508c2cb95197e550f54c6bc03cfd4668ef3c9f1011ec3950037cb4b11",
    "installer": {
        "script": [
            "if ($global) { install-python-package --global --quiet --dir $dir --repo https://github.com/jaraco/keyring.git --branch \"v$version\" --clone-dir $app }",
            "else { install-python-package --quiet --dir $dir --repo https://github.com/jaraco/keyring.git --branch \"v$version\" --clone-dir $app }",
            "if ($LASTEXITCODE) { exit 1 }"
        ]
    },
    "uninstaller": {
        "script": [
            "if ($global) { uninstall-python-package $app --global --dir $dir }",
            "else { uninstall-python-package $app --dir $dir }"
        ]
    },
    "checkver": {
        "url": "https://api.github.com/repos/jaraco/keyring/tags",
        "regex": "v([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://raw.githubusercontent.com/TheRandomLabs/Scoop-Python/master/placeholder.txt"
    }
}
