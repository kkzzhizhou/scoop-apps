{
    "version": "4.12.0-0",
    "description": "A conda-forge distribution",
    "homepage": "https://mirrors.tuna.tsinghua.edu.cn/github-release/conda-forge/miniforge/",
    "license": "BSD-3-Clause",
    "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/conda-forge/miniforge/Miniforge3-4.12.0-0/Mambaforge-4.12.0-0-Windows-x86_64.exe",
    "hash": "237f52e3a68eb3278d3c837763267840f23444e1b3762c00b1b8f01289d649c9",
    "autoupdate": {
        "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/conda-forge/miniforge/Miniforge3-$version/Mambaforge-$version-Windows-x86_64.exe",
        "hash": {
            "url": "$baseurl",
            "regex": "(?sm)$basename<.*?<td>$md5</td>"
        }
    },
    "installer": {
        "args": [
            "/S",
            "/InstallationType=JustMe",
            "/RegisterPython=0",
            "/AddToPath=0",
            "/NoRegistry=1",
            "/D=$dir"
        ]
    },
    "uninstaller": {
        "file": "Uninstall-Mambaforge.exe",
        "args": "/S"
    },
    "bin": [
        "python.exe",
        "pythonw.exe",
        [
            "python.exe",
            "python3"
        ]
    ],
    "persist": "envs",
    "env_add_path": [
        "scripts",
        "Library\\bin"
    ],
    "checkver": {
        "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/conda-forge/miniforge/",
        "regex": "Miniforge3-([\\d.]+-\\d)"
    }
}
