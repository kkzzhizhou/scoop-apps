    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-fa",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox (fa)",
            "-profile \"$dir\\profile\""
        ]
    ],
