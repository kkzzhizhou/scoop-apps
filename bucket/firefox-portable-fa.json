    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox (fa)",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "Profile",
    "checkver": {
        "url": "https://www.mozilla.org/fa-IR/firefox/notes/",
        "regex": "<div class=\"c-release-version\">([\\d.]+)</div>"
    },
    "autoupdate": {
        "architecture": {
