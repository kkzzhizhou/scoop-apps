        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/130.0/win32/fa-IR/Firefox%20Setup%20130.0.exe#/dl.7z",
            "hash": "sha512:9b7f89435304f98ea3e3646cac1a34505adaf981fa356b8abc6cffc325d3faf872f08372c94bdc47f2354662719d8c37d6bc5596cab730f17e39e42af1e0800a"
