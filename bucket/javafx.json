{
    "description": "OpenJFX is an open source, next generation client application platform for desktop, mobile and embedded systems built on Java.(Scoop bucket for Cybersecurity by s0nd9r)",
    "homepage": "https://openjfx.io/",
    "version": "17.0.12",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0",
    "notes": "Scoop bucket for Cybersecurity by s0nd9r(https://github.com/s0nd9r/scoop-security).",
    "suggest": {
        "JDK": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://download2.gluonhq.com/openjfx/17.0.12/openjfx-17.0.12_windows-x64_bin-sdk.zip",
            "hash": "d1c31cd9d194e643cfbfc5087a6ab584a64e8b52fb4cf328d7cd7eb06dac0661"
        }
    },
    "extract_dir": "javafx-sdk-17.0.12",
    "env_set": {
        "PATH_TO_FX": "$dir\\lib"
    },
    "checkver": {
        "url": "https://gluonhq.com/wp-content/uploads/custom-css-js/119502.js",
        "regex": "LTS,\\s*(\\d+),\\s*(\\d+),\\s*(\\d+).*?#lts17",
        "replace": "${1}.${2}.${3}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download2.gluonhq.com/openjfx/$version/openjfx-$version_windows-x64_bin-sdk.zip"
            }
        },
        "extract_dir": "javafx-sdk-$version",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
