        [
            "zenmap.exe",
            "Nmap - Zenmap GUI"
        ]
    ],
    "notes": "'Npcap' has been installed automatically.",
    "checkver": {
        "url": "https://nmap.org/download.html",
        "regex": "nmap-([\\d.]+)-setup.exe"
    },
    "autoupdate": {
        "url": "https://nmap.org/dist/nmap-$version-setup.exe#/setup.exe"
