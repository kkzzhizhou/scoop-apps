            "hash": "945b07182de7de279ba5ae5c746785dfd55cf9b17481d3535b0b03efd6e64567"
        },
        "32bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.8.19/uv-i686-pc-windows-msvc.zip",
            "hash": "32c7176e49f5fa1c7480b32e325a631431212904b1020427e62afef46fddddb9"
        },
        "arm64": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.8.19/uv-aarch64-pc-windows-msvc.zip",
            "hash": "3bfe2db4bccf95e05d2685bcbfad7e49e7cd2177a20aebe81a5e5348cbdfc0a3"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe",
        "uvw.exe"
    ],
    "env_set": {
        "UV_CACHE_DIR": "$persist_dir\\caches",
        "UV_PYTHON_CACHE_DIR": "$dir\\python\\caches",
        "UV_INSTALL_DIR": "$dir\\bin",
        "UV_PYTHON_INSTALL_DIR": "$dir\\python\\versions",
        "UV_PYTHON_BIN_DIR": "$dir\\python\\bin",
        "UV_TOOL_BIN_DIR": "$dir\\tools\\bin",
        "UV_TOOL_DIR": "$dir\\tools"
    },
    "env_add_path": [
        "bin",
        "python\\versions",
        "python\\bin",
        "tools\\bin"
    ],
    "persist": [
        "bin",
        "python",
        "tools"
    ],
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-x86_64-pc-windows-msvc.zip"
            },
            "32bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-i686-pc-windows-msvc.zip"
            },
            "arm64": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-aarch64-pc-windows-msvc.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
