{
    "version": "0.9.1",
    "description": "An extremely fast Python package installer and resolver, written in Rust.",
    "homepage": "https://docs.astral.sh/uv/",
    "license": "Apache-2.0|MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.9.1/uv-x86_64-pc-windows-msvc.zip",
            "hash": "8dba20107ebcd38f3a8e4acfeb7b209d594e112a3be8d1cea69fe6d501a69fdf"
        },
        "32bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.9.1/uv-i686-pc-windows-msvc.zip",
            "hash": "8880c92097c5410c3bbad5e72cf7d38b57523ed374bcd7462d309b9a11b6df33"
        },
        "arm64": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.9.1/uv-aarch64-pc-windows-msvc.zip",
            "hash": "862e0fef8e68f229c99ce05de70a712221bc552403f480379d03f5b05a6886fe"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe",
        "uvw.exe"
    ],
    "env_set": {
        "UV_CACHE_DIR": "$persist_dir\\caches",
        "UV_PYTHON_CACHE_DIR": "$dir\\python\\caches",
        "UV_INSTALL_DIR": "$dir\\bin",
        "UV_PYTHON_INSTALL_DIR": "$dir\\python\\versions",
        "UV_PYTHON_BIN_DIR": "$dir\\python\\bin",
        "UV_TOOL_BIN_DIR": "$dir\\tools\\bin",
        "UV_TOOL_DIR": "$dir\\tools"
    },
    "env_add_path": [
        "bin",
        "python\\versions",
        "python\\bin",
        "tools\\bin"
    ],
    "persist": [
        "bin",
        "python",
        "tools"
    ],
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-x86_64-pc-windows-msvc.zip"
            },
            "32bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-i686-pc-windows-msvc.zip"
            },
            "arm64": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-aarch64-pc-windows-msvc.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
