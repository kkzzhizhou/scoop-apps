{
    "version": "0.9.2",
    "description": "An extremely fast Python package installer and resolver, written in Rust.",
    "homepage": "https://docs.astral.sh/uv/",
    "license": "Apache-2.0|MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.9.2/uv-x86_64-pc-windows-msvc.zip",
            "hash": "ba61d01b7470b282e09e7c82a053472723c2a737fb3d137bd0f111e420cc1d9f"
        },
        "32bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.9.2/uv-i686-pc-windows-msvc.zip",
            "hash": "f0cc299f9cfd9850d30f073794cda9523ac7fa2b90de3d1ed47e3fdcbda69ab6"
        },
        "arm64": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.9.2/uv-aarch64-pc-windows-msvc.zip",
            "hash": "b28ae90188f00c6d9e6f32fe252ace25a3698eeca1458d435ac1ca55049607ff"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe",
        "uvw.exe"
    ],
    "env_set": {
        "UV_CACHE_DIR": "$persist_dir\\caches",
        "UV_PYTHON_CACHE_DIR": "$dir\\python\\caches",
        "UV_INSTALL_DIR": "$dir\\bin",
        "UV_PYTHON_INSTALL_DIR": "$dir\\python\\versions",
        "UV_PYTHON_BIN_DIR": "$dir\\python\\bin",
        "UV_TOOL_BIN_DIR": "$dir\\tools\\bin",
        "UV_TOOL_DIR": "$dir\\tools"
    },
    "env_add_path": [
        "bin",
        "python\\versions",
        "python\\bin",
        "tools\\bin"
    ],
    "persist": [
        "bin",
        "python",
        "tools"
    ],
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-x86_64-pc-windows-msvc.zip"
            },
            "32bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-i686-pc-windows-msvc.zip"
            },
            "arm64": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-aarch64-pc-windows-msvc.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
