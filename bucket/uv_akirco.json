{
    "version": "0.9.6",
    "description": "An extremely fast Python package installer and resolver, written in Rust.",
    "homepage": "https://docs.astral.sh/uv/",
    "license": "Apache-2.0|MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.9.6/uv-x86_64-pc-windows-msvc.zip",
            "hash": "4cde4a7907214053320d5ab4b6f9add77b7aca6eb39cc7e4928b61704c1328b1"
        },
        "32bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.9.6/uv-i686-pc-windows-msvc.zip",
            "hash": "7a08b77907b5a5e93f0419be197449747ad1d5647268ff76b50c9f21904459f2"
        },
        "arm64": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.9.6/uv-aarch64-pc-windows-msvc.zip",
            "hash": "57370b6a43b9c7d93064de27e5ef7e068868062473df227a733e2d8a46ea87cf"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe",
        "uvw.exe"
    ],
    "env_set": {
        "UV_CACHE_DIR": "$persist_dir\\caches",
        "UV_PYTHON_CACHE_DIR": "$dir\\python\\caches",
        "UV_INSTALL_DIR": "$dir\\bin",
        "UV_PYTHON_INSTALL_DIR": "$dir\\python\\versions",
        "UV_PYTHON_BIN_DIR": "$dir\\python\\bin",
        "UV_TOOL_BIN_DIR": "$dir\\tools\\bin",
        "UV_TOOL_DIR": "$dir\\tools"
    },
    "env_add_path": [
        "bin",
        "python\\versions",
        "python\\bin",
        "tools\\bin"
    ],
    "persist": [
        "bin",
        "python",
        "tools"
    ],
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-x86_64-pc-windows-msvc.zip"
            },
            "32bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-i686-pc-windows-msvc.zip"
            },
            "arm64": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-aarch64-pc-windows-msvc.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
