{
    "version": "2.1",
    "description": "该脚本用于检查Apache Tomcat管理页面的弱密码，并尝试通过上传自定义WAR包部署Webshell",
    "homepage": "https://github.com/lizhianyuguangming/TomcatWeakPassChecker",
    "license": "",
    "suggest": {
        "Python 3.7+": "python"
    },
    "url": "https://github.com/lizhianyuguangming/TomcatWeakPassChecker/releases/download/TomcatWeakPassCheckerv2.1/TomcatWeakPassChecker2.1.zip",
    "hash": "",
    "pre_install": "Set-Content \"$dir\\TomcatWeakPassChecker.bat\" '@pushd %~dp0\npython3 \"TomcatWeakPassChecker$version.py\" %*\n@popd' -Encoding Ascii",
    "post_install": [
        "Move-Item -Path \"$dir\\TomcatWeakPassChecker-$version\\*\" -Destination \"$dir\\\"",
        "Remove-Item \"$dir\\TomcatWeakPassChecker-$version\" -Force -Recurse",
        "pip3 install -r \"$dir\\requirements.txt\" -i https://pypi.tuna.tsinghua.edu.cn/simple"
    ],
    "bin": "TomcatWeakPassChecker.bat",
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/lizhianyuguangming/TomcatWeakPassChecker/releases/download/TomcatWeakPassCheckerv$version/TomcatWeakPassChecker$version.zip"
    }
}