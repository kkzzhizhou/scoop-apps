        ]
    ],
    "shortcuts": [
        [
            "pwsh.exe",
            "PowerShell Core"
        ]
    ],
    "checkver": {
        "github": "https://github.com/PowerShell/PowerShell"
    },
    "autoupdate": {
        "architecture": {
