{
    "version": "2.0.6",
    "description": "A comprehensive benchmarking software that supports 64-bit (x64/ARM64), many-core, and multilingual (48+ languages).",
    "homepage": "https://crystalmark.info/en/software/crystalmarkretro/",
    "license": "MIT",
    "depends": "Scoop4kariiin/Scoop4kariiinUtils",
    "url": "https://zenlayer.dl.sourceforge.net/project/crystalmarkretro/2.0.6/CrystalMarkRetro2_0_6Shizuku.zip",
    "hash": "sha1:b149c57bf189dc0171de433f58b89dfb5030d4fd",
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "CrystalMarkRetro64S.exe",
                    "CrystalMark Retro Shizuku Edition"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "CrystalMarkRetro32S.exe",
                    "CrystalMark Retro Shizuku Edition"
                ]
            ]
        },
        "arm64": {
            "shortcuts": [
                [
                    "CrystalMarkRetroA64S.exe",
                    "CrystalMark Retro Shizuku Edition"
                ]
            ]
        }
    },
    "pre_install": [
        "'CrystalMarkRetro32S.ini', 'CrystalMarkRetro64S.ini', 'CrystalMarkRetroA64S.ini' | ForEach-Object {",
        "    if (-not (Test-Path \"$persist_dir\\$_\")) {",
        "        New-Item \"$dir\\$_\" -ItemType File | Out-Null",
        "    }",
        "}"
    ],
    "persist": [
        "CrystalMarkRetro32S.ini",
        "CrystalMarkRetro64S.ini",
        "CrystalMarkRetroA64S.ini"
    ],
    "checkver": {
        "url": "https://crystalmark.info/en/download/",
        "regex": "CrystalMark Retro ([\\w\\.]+) &#8211;"
    },
    "autoupdate": {
        "url": "https://zenlayer.dl.sourceforge.net/project/crystalmarkretro/$version/CrystalMarkRetro$underscoreVersionShizuku.zip"
    }
}
