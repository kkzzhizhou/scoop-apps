{
    "version": "2.13.0",
    "description": "A simple and extensible shell script for managing your todo.txt file.",
    "homepage": "http://todotxt.org/",
    "license": "GPL-3.0",
    "suggest": {
        "git": "git"
    },
    "url": "https://github.com/todotxt/todo.txt-cli/releases/download/v2.13.0/todo.txt_cli-2.13.0.zip",
    "hash": "5c8b2944ea1ee396b3babb170b878731545612224103ca415188705d8e013a8f",
    "extract_dir": "todo.txt_cli-2.13.0",
    "installer": {
        "script": "Set-Content \"$dir\\todo.bat\" \"@for /f %%i in ('scoop prefix git') do @%%i\\bin\\bash.exe %~dp0\\todo.sh %*\" -Encoding ASCII"
    },
    "bin": "todo.bat",
    "persist": "todo.cfg",
    "checkver": {
        "github": "https://github.com/todotxt/todo.txt-cli"
    },
    "autoupdate": {
        "url": "https://github.com/todotxt/todo.txt-cli/releases/download/v$version/todo.txt_cli-$version.zip",
        "extract_dir": "todo.txt_cli-$version"
    }
}
