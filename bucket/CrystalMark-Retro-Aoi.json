{
    "version": "2.0.7",
    "description": "A comprehensive benchmarking software that supports 64-bit (x64/ARM64), many-core, and multilingual (48+ languages).",
    "homepage": "https://crystalmark.info/en/software/crystalmarkretro/",
    "license": "MIT",
    "url": "https://zenlayer.dl.sourceforge.net/project/crystalmarkretro/2.0.7/CrystalMarkRetro2_0_7Aoi.zip",
    "hash": "sha1:6f27772b4864e61f995586f4d88c3f00510a7dbd",
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "CrystalMarkRetro64A.exe",
                    "CrystalMark Retro Aoi Edition"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "CrystalMarkRetro32A.exe",
                    "CrystalMark Retro Aoi Edition"
                ]
            ]
        },
        "arm64": {
            "shortcuts": [
                [
                    "CrystalMarkRetroA64A.exe",
                    "CrystalMark Retro Aoi Edition"
                ]
            ]
        }
    },
    "pre_install": [
        "'CrystalMarkRetro32A.ini', 'CrystalMarkRetro64A.ini', 'CrystalMarkRetroA64A.ini' | ForEach-Object {",
        "    if (-not (Test-Path \"$persist_dir\\$_\")) {",
        "        New-Item \"$dir\\$_\" -ItemType File | Out-Null",
        "    }",
        "}"
    ],
    "persist": [
        "CrystalMarkRetro32A.ini",
        "CrystalMarkRetro64A.ini",
        "CrystalMarkRetroA64A.ini"
    ],
    "checkver": {
        "url": "https://crystalmark.info/en/download/",
        "regex": "CrystalMark Retro ([\\w\\.]+) &#8211;"
    },
    "autoupdate": {
        "url": "https://zenlayer.dl.sourceforge.net/project/crystalmarkretro/$version/CrystalMarkRetro$underscoreVersionAoi.zip"
    }
}
