{
    "version": "0.6.1",
    "description": "An open-source AI agent that lives in your terminal (with bun)",
    "homepage": "https://qwenlm.github.io/qwen-code-docs",
    "license": "Apache-2.0",
    "url": "http://www.gstatic.com/generate_204#/dummy",
