    "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\Uninstall.exe\" -Recurse"
  ],
  "bin": [
    "nmap.exe",
    "ncat.exe",
    "ndiff.exe",
    "zenmap.exe",
    "nping.exe"
  ],
  "shortcuts": [
    [
      "zenmap.exe",
      "Nmap - Zenmap GUI"
    ]
  ],
  "checkver": {
    "url": "https://nmap.org/download.html",
    "regex": "nmap-([\\d.]+)-setup\\.exe"
  },
  "autoupdate": {
    "url": "https://nmap.org/dist/nmap-$version-setup.exe#/dl.7z"
  }
}