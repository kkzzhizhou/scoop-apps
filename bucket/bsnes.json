        "settings.bml"
    ],
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\settings.bml\")) {",
        "    New-Item \"$dir\\settings.bml\" -Type File | Out-Null",
        "}"
    ],
    "shortcuts": [
        [
            "bsnes.exe",
            "bsnes"
        ]
    ]
