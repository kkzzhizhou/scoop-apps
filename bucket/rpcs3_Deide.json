{
    "homepage": "https://rpcs3.net/",
    "version": "0.0.39-18742",
    "description": "Playstation 3 Emulator",
    "license": {
        "identifier": "GPL-2.0",
        "url": "https://github.com/RPCS3/rpcs3/blob/master/LICENSE"
    },
    "url": "https://github.com/RPCS3/rpcs3-binaries-win/releases/download/build-b7311bd6dbea76af058b05849c69406f2a7d744e/rpcs3-v0.0.39-18742-b7311bd6_win64_msvc.7z",
    "hash": "f849d74361e1b2aa0a7ea717857ca6041e83fff6dc3cec6eeff5280320e0d64d",
    "bin": "rpcs3.exe",
    "shortcuts": [
        [
            "rpcs3.exe",
            "PS3"
        ]
    ],
    "persist": [
        "dev_flash",
        "dev_flash2",
        "dev_flash3",
        "dev_hdd0",
        "dev_hdd1",
        "cache",
        "config",
        "patches",
        "captures",
        "recordings",
        "savestates",
        "config.yml",
        "games.yml"
    ],
    "installer": {
        "script": [
            "function create($path, $type, $content) {",
            "   if (-not (Test-Path \"$path\")) {",
            "       New-Item \"$path\" -ItemType $type -Value $content | Out-Null",
            "       Write-Output \"Creating $path...\"",
            "   }",
            "}",
            "$conf = \"$persist_dir\\config.yml\"",
            "$games = \"$persist_dir\\games.yml\"",
            "create $conf File",
            "create $games File"
        ]
    },
    "checkver": {
        "url": "https://update.rpcs3.net/?api",
        "jsonpath": {
            "version": "$.latest_build.version",
            "download": "$.latest_build.windows.download",
            "hash": "$.latest_build.windows.checksum"
        }
    },
    "autoupdate": {
        "url": "$matchDownload",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
