{
    "homepage": "https://rpcs3.net/",
    "version": "0.0.38-18451",
    "description": "Playstation 3 Emulator",
    "license": {
        "identifier": "GPL-2.0",
        "url": "https://github.com/RPCS3/rpcs3/blob/master/LICENSE"
    },
    "url": "https://github.com/RPCS3/rpcs3-binaries-win/releases/download/build-3f6529fecbacdf90aa556ae0fd39b478ae28c27f/rpcs3-v0.0.38-18451-3f6529fe_win64_msvc.7z",
    "hash": "d2b0a5f73eee609dcf02c1eeb1c689feb24b17f7f2f4a1f212fe765a4d701600",
    "bin": "rpcs3.exe",
    "shortcuts": [
        [
            "rpcs3.exe",
            "PS3"
        ]
    ],
    "persist": [
        "dev_flash",
        "dev_flash2",
        "dev_flash3",
        "dev_hdd0",
        "dev_hdd1",
        "cache",
        "config",
        "patches",
        "captures",
        "recordings",
        "savestates",
        "config.yml",
        "games.yml"
    ],
    "installer": {
        "script": [
            "function create($path, $type, $content) {",
            "   if (-not (Test-Path \"$path\")) {",
            "       New-Item \"$path\" -ItemType $type -Value $content | Out-Null",
            "       Write-Output \"Creating $path...\"",
            "   }",
            "}",
            "$conf = \"$persist_dir\\config.yml\"",
            "$games = \"$persist_dir\\games.yml\"",
            "create $conf File",
            "create $games File"
        ]
    },
    "checkver": {
        "url": "https://update.rpcs3.net/?api",
        "jsonpath": {
            "version": "$.latest_build.version",
            "download": "$.latest_build.windows.download",
            "hash": "$.latest_build.windows.checksum"
        }
    },
    "autoupdate": {
        "url": "$matchDownload",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
