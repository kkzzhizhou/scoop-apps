{
    "version": "0.9.7",
    "description": "An extremely fast Python package installer and resolver, written in Rust.",
    "homepage": "https://docs.astral.sh/uv/",
    "license": "Apache-2.0|MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.9.7/uv-x86_64-pc-windows-msvc.zip",
            "hash": "5d250c32d3604e28dbe18dc65c668ff628c53e00dde2c642576e831e4a60da64"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe",
        "uvw.exe"
    ],
    "persist": [
        "cache",
        "tool",
        "python"
    ],
    "env_set": {
        "UV_CACHE_DIR": "$persist_dir\\cache",
        "UV_TOOL_DIR": "$persist_dir\\tool",
        "UV_TOOL_BIN_DIR": "$scoopdir\\shims",
        "UV_DEFAULT_INDEX": "https://mirrors.aliyun.com/pypi/simple/",
        "UV_PYTHON_INSTALL_DIR": "$persist_dir\\python",
        "UV_PYTHON_INSTALL_MIRROR": "https://mirror.nju.edu.cn/github-release/astral-sh/python-build-standalone/"
    },
    "##": "更多环境变量设置 详见：https://docs.astral.sh/uv/reference/environment/",
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-x86_64-pc-windows-msvc.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
