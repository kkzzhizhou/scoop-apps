    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/98.0b3/win64/de-DE/Firefox%20Setup%2098.0b3.exe#/dl.7z",
            "hash": "sha512:04ab2f626ede0922fe57f1bc3ee7d20c5765818cf8fe02e38aa743d6d65f511787bd1b572907550040eb76923c4247e32ab10704691f136ae87cba5949b1ce4e"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/98.0b3/win32/de-DE/Firefox%20Setup%2098.0b3.exe#/dl.7z",
            "hash": "sha512:04ab2f626ede0922fe57f1bc3ee7d20c5765818cf8fe02e38aa743d6d65f511787bd1b572907550040eb76923c4247e32ab10704691f136ae87cba5949b1ce4e"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta-de",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta (de)",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/de-DE/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/de-DE/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/de-DE/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/de-DE/$basename"
                }
            }
        }
    }
}
