    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta (de)",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
