    "homepage": "https://www.mozilla.org/de-DE/firefox/beta/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/118.0b2/win64/de-DE/Firefox%20Setup%20118.0b2.exe#/dl.7z",
            "hash": "sha512:3ce1ba3f84bb376f0ba797341e774abb65214d2dbe88b070ea87b1b90f6c428074ab2113b26b1dc1103b2efa6396151325a6b81b818a7db3260329b9272185ef"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/118.0b2/win32/de-DE/Firefox%20Setup%20118.0b2.exe#/dl.7z",
            "hash": "sha512:3ce1ba3f84bb376f0ba797341e774abb65214d2dbe88b070ea87b1b90f6c428074ab2113b26b1dc1103b2efa6396151325a6b81b818a7db3260329b9272185ef"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta-de",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta (de)",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/de-DE/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/de-DE/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/de-DE/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/de-DE/$basename"
                }
            }
        }
    }
}
