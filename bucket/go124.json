{
    "version": "1.24.8",
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "homepage": "https://golang.org",
    "license": "BSD-3-Clause",
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.24.8.windows-amd64.zip",
            "hash": "0de7b65422d9377404a22f3903f17b20e4065f6c43e136ae2de16494c8c6b057"
        },
        "32bit": {
            "url": "https://dl.google.com/go/go1.24.8.windows-386.zip",
            "hash": "c6430cb4aff0d96a7df1d55d4e7003af35b30d04db0e95e848d0772a9f27978e"
        },
        "arm64": {
            "url": "https://dl.google.com/go/go1.24.8.windows-arm64.zip",
            "hash": "da6d3dbed0d7923605f3656d4213915b0f148797d18e18cde2a654a6f232b6d5"
        }
    },
    "extract_dir": "go",
    "installer": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Adding '$envgopath\\bin' to PATH...\"",
            "Add-Path -Path \"$envgopath\\bin\" -Global:$global -Force"
        ]
    },
    "uninstaller": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Removing '$envgopath\\bin' from PATH...\"",
            "Remove-Path -Path \"$envgopath\\bin\" -Global:$global"
        ]
    },
    "bin": [
        "bin\\go.exe",
        "bin\\gofmt.exe"
    ],
    "checkver": {
        "url": "https://golang.org/dl/",
        "regex": "go(1\\.24\\.\\d+)\\.windows-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/go/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://dl.google.com/go/go$version.windows-386.zip"
            },
            "arm64": {
                "url": "https://dl.google.com/go/go$version.windows-arm64.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
