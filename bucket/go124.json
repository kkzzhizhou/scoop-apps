    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.24.9.windows-amd64.zip",
            "hash": "dce0a479fa60142aebb67e22664dfae156d4ec1dd1ab314b7a2c27d4ee59681c"
        },
        "32bit": {
            "url": "https://dl.google.com/go/go1.24.9.windows-386.zip",
            "hash": "ef5e4276031603f3ef8c15ba836118f7f76b6e4ca2339686935ccc71d9c196b8"
        },
        "arm64": {
            "url": "https://dl.google.com/go/go1.24.9.windows-arm64.zip",
            "hash": "2cfd1d3ad30e4fef556e8c5b743eb69c8b5045557dca2deb4a156e162d4c8baf"
        }
    },
    "extract_dir": "go",
    "installer": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Adding '$envgopath\\bin' to PATH...\"",
            "Add-Path -Path \"$envgopath\\bin\" -Global:$global -Force"
        ]
    },
    "uninstaller": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Removing '$envgopath\\bin' from PATH...\"",
            "Remove-Path -Path \"$envgopath\\bin\" -Global:$global"
        ]
    },
    "bin": [
        "bin\\go.exe",
        "bin\\gofmt.exe"
    ],
    "checkver": {
        "url": "https://golang.org/dl/",
        "regex": "go(1\\.24\\.\\d+)\\.windows-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/go/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://dl.google.com/go/go$version.windows-386.zip"
            },
            "arm64": {
                "url": "https://dl.google.com/go/go$version.windows-arm64.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
