{
    "version": "6.0.3.2",
    "description": "A modern media player for Windows that works just like mpv",
    "homepage": "https://github.com/mpvnet-player/mpv.net/",
    "license": "GPL-2.0-or-later",
    "architecture": {
        "64bit": {
            "url": "https://github.com/mpvnet-player/mpv.net/releases/download/v6.0.3.2-beta/mpv.net-6.0.3.2-beta.zip",
            "hash": "81975f82de761320e06f11f59a6983348c64d110f89a8a1779ba8a4a80813176"
        }
    },
    "bin": "mpvnet.com",
    "shortcuts": [
        [
            "mpvnet.exe",
            "mpv.net"
        ]
    ],
    "persist": "portable_config",
    "checkver": {
        "url": "https://api.github.com/repos/mpvnet-player/mpv.net/releases",
        "jsonpath": "$..browser_download_url",
        "regex": "download/v(?<tag>[\\d.]+(?<channel>-beta)?)/mpv.net-([\\d.]+)-beta.zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/mpvnet-player/mpv.net/releases/download/v$matchTag/mpv.net-$version-beta.zip"
            }
        }
    }
}
