{
    "$schema": "https://raw.githubusercontent.com/ScoopInstaller/Scoop/master/schema.json",
    "homepage": "https://github.com/kiennq/emacs-build",
    "license": "GPLv3",
    "version": "31.358.20250601.16108d0",
    "url": "https://github.com/kiennq/emacs-build/releases/download/v31.358.20250601.16108d0/emacs-16108d0-ucrt-x86_64-mps.msix",
    "hash": "26cea402086c713933606d5c7cb8e82e9f7c04b011c51ac048d42dd1f17166d2",
    "installer": {
        "script": [
            "Get-ChildItem \"$dir\\*.msix\" | %{Expand-7zipArchive $_.FullName \"$dir\"}",
            "Remove-Item -Force \"$dir\\*.msix\"",
            "Push-Location \"$dir\"",
            "dir 'bin\\*%2b*' | %{ren \"$_\" \"$($_.Name -replace '%2b','+')\"}",
            "Pop-Location"
        ]
    },
    "bin": [
        "bin\\runemacs.exe",
        "bin\\emacs.exe",
        "bin\\emacsclient.exe",
        "bin\\emacsclientw.exe",
        [
            "bin\\emacsclientw.exe",
            "emw",
            "-c -n -a \"\""
        ]
    ],
    "checkver": {
        "github": "https://github.com/kiennq/emacs-build",
        "regex": "/tag/v([\\d\\w.-]+)"
    },
    "autoupdate": {
        "url": "https://github.com/kiennq/emacs-build/releases/download/v$version/emacs-$buildVersion-ucrt-x86_64-mps.msix"
    },
    "shortcuts": [
        [
            "bin\\runemacs.exe",
            "Emacs"
        ],
        [
            "bin\\emacsclientw.exe",
            "Emacs Client",
            "-c -n -a \"\""
        ]
    ]
}
