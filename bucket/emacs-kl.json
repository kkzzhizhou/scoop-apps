{
    "$schema": "https://raw.githubusercontent.com/ScoopInstaller/Scoop/master/schema.json",
    "homepage": "https://github.com/kiennq/emacs-build",
    "license": "GPLv3",
    "version": "31.371.20250929.35119d2",
    "url": "https://github.com/kiennq/emacs-build/releases/download/v31.371.20250929.35119d2/emacs-35119d2-ucrt-x86_64-mps.msix",
    "hash": "e0e20887efc1d201bcb54db3484dbfbb48d6d6c15edace9ed43b6fce5f64e8e8",
    "installer": {
        "script": [
            "Get-ChildItem \"$dir\\*.msix\" | %{Expand-7zipArchive $_.FullName \"$dir\"}",
            "Remove-Item -Force \"$dir\\*.msix\"",
            "Push-Location \"$dir\"",
            "dir 'bin\\*%2b*' | %{ren \"$_\" \"$($_.Name -replace '%2b','+')\"}",
            "Pop-Location"
        ]
    },
    "bin": [
        "bin\\runemacs.exe",
        "bin\\emacs.exe",
        "bin\\emacsclient.exe",
        "bin\\emacsclientw.exe",
        [
            "bin\\emacsclientw.exe",
            "emw",
            "-c -n -a \"\""
        ]
    ],
    "checkver": {
        "github": "https://github.com/kiennq/emacs-build",
        "regex": "/tag/v([\\d\\w.-]+)"
    },
    "autoupdate": {
        "url": "https://github.com/kiennq/emacs-build/releases/download/v$version/emacs-$buildVersion-ucrt-x86_64-mps.msix"
    },
    "shortcuts": [
        [
            "bin\\runemacs.exe",
            "Emacs"
        ],
        [
            "bin\\emacsclientw.exe",
            "Emacs Client",
            "-c -n -a \"\""
        ]
    ]
}
