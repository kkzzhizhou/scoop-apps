{
    "$schema": "https://raw.githubusercontent.com/ScoopInstaller/Scoop/master/schema.json",
    "homepage": "https://github.com/kiennq/emacs-build",
    "license": "GPLv3",
    "version": "31.357.20250525.34bdcae",
    "url": "https://github.com/kiennq/emacs-build/releases/download/v31.357.20250525.34bdcae/emacs-34bdcae-ucrt-x86_64-mps.msix",
    "hash": "18b6e8681c58c9727e7e624a1aa3f6bdf25170e33a9bbec4c8bfe395a7557f45",
    "installer": {
        "script": [
            "Get-ChildItem \"$dir\\*.msix\" | %{Expand-7zipArchive $_.FullName \"$dir\"}",
            "Remove-Item -Force \"$dir\\*.msix\"",
            "Push-Location \"$dir\"",
            "dir 'bin\\*%2b*' | %{ren \"$_\" \"$($_.Name -replace '%2b','+')\"}",
            "Pop-Location"
        ]
    },
    "bin": [
        "bin\\runemacs.exe",
        "bin\\emacs.exe",
        "bin\\emacsclient.exe",
        "bin\\emacsclientw.exe",
        [
            "bin\\emacsclientw.exe",
            "emw",
            "-c -n -a \"\""
        ]
    ],
    "checkver": {
        "github": "https://github.com/kiennq/emacs-build",
        "regex": "/tag/v([\\d\\w.-]+)"
    },
    "autoupdate": {
        "url": "https://github.com/kiennq/emacs-build/releases/download/v$version/emacs-$buildVersion-ucrt-x86_64-mps.msix"
    },
    "shortcuts": [
        [
            "bin\\runemacs.exe",
            "Emacs"
        ],
        [
            "bin\\emacsclientw.exe",
            "Emacs Client",
            "-c -n -a \"\""
        ]
    ]
}
