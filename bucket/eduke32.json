    "autoupdate": {
        "architecture": {
            "32bit": {
                "url": "https://dukeworld.com/eduke32/synthesis/$version/eduke32_win32_$version.7z"
            },
            "64bit": {
                "url": "https://dukeworld.com/eduke32/synthesis/$version/eduke32_win64_$version.7z"
            }
        }
    }
