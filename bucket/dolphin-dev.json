    "persist": "User",
    "checkver": {
        "url": "https://dolphin-emu.org/download/",
        "regex": "(?<prefix1>.{5})/dolphin-master-(?<version>[\\w-]+)-x64[\\s\\S]+?(?<prefix2>.{5})/dolphin-master-\\k<version>-ARM64"
    },
    "autoupdate": {
