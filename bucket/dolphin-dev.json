    "license": {
        "identifier": "GPL-2.0-or-later",
        "url": "https://github.com/dolphin-emu/dolphin/blob/master/COPYING"
    },
    "suggest": {
        "Microsoft Visual C++ Runtime 2022": "extras/vcredist2022"
    },
    "architecture": {
        "64bit": {
            "url": "https://dl.dolphin-emu.org/builds/58/a8/dolphin-master-2506-160-x64.7z",
            "hash": "f08a67b3ae02c3e1ab73604323bef30993dc710dc2b27f7ba7a9d9607b1f3de5",
            "extract_dir": "Dolphin-x64"
        },
        "arm64": {
