    "homepage": "https://www.mozilla.org/en-US/firefox/beta/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/116.0b3/win64/en-US/Firefox%20Setup%20116.0b3.exe#/dl.7z",
            "hash": "sha512:371dde66ccc6b2b37acb9c3f00d08ab106166a3bab75f2b34ee401ec4892e37352a054e75d53eeac0f3ff0da7de76a707c05ac93cec8709797393d40b5287f38"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/116.0b3/win32/en-US/Firefox%20Setup%20116.0b3.exe#/dl.7z",
            "hash": "sha512:a0c6212b2d223b4b1c07b8faba931d76eff844b785152f828a1cbca0b34e86248dbe76c60db21db935d2e0a383af90d1ac621af0a1a1d5bafe981811fe5a8db8"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
