    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/109.0b4/win64/en-US/Firefox%20Setup%20109.0b4.exe#/dl.7z",
            "hash": "sha512:4f4328758d92ff7f1bdacfc97f46b0e86598c7dada2f8d8c18552bed4371505d648be1a5a634327fa57d011778cf9c9db0683172d5acbe57ad58ed879f3b3250"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/109.0b4/win32/en-US/Firefox%20Setup%20109.0b4.exe#/dl.7z",
            "hash": "sha512:0c54bf78f524d7fdc6f307942f37b01bd7dec702c23be01948ef16503c652df02f9e73aba835ea85ebc2bac76e8e14bcede99b992629a5e0eea9310b5f2c41b7"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
