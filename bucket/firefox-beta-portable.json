    "homepage": "https://www.mozilla.org/en-US/firefox/beta/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/114.0b4/win64/en-US/Firefox%20Setup%20114.0b4.exe#/dl.7z",
            "hash": "sha512:a21eb98d760e3cc4a9ecedae183e08feaec50aa124b87b20436815beca7cc41d3fa26ffa0b81750f2cf048130f2f3b52970cd9cbf7a907a22406c7883574ec76"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/114.0b4/win32/en-US/Firefox%20Setup%20114.0b4.exe#/dl.7z",
            "hash": "sha512:ff083a209b603764dd77bd2502969adefb39d1541fac86af77f7dc35c2196c627a3e7feb5a38d3c322cc20fa728467a8b9212c5df92b1cddba2cffa5c0cb2fb0"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
