    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/102.0b3/win64/en-US/Firefox%20Setup%20102.0b3.exe#/dl.7z",
            "hash": "sha512:d05301bacda6eb3f6b9cc7026f88176120abaa9e61a259a2e428a6a9af56d8eaf813ed1378a316183dc66f2285717247ef377547223d62f90bcae48dcf4be92f"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/102.0b3/win32/en-US/Firefox%20Setup%20102.0b3.exe#/dl.7z",
            "hash": "sha512:12e05e50fdbc3bac4570ad4a0653a67130afba3b7d51235e396c181380d19cdc16a58bc0c54abae72ecdf5a4068a73f510f5a8ed79ffd512795777e23ba7601c"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
