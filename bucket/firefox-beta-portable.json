    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/109.0b2/win64/en-US/Firefox%20Setup%20109.0b2.exe#/dl.7z",
            "hash": "sha512:bd9c5e5e40009106e53a5fdb7bee6112f00a6450e2d6602a4383d916c05819fae61eb917a9450b692cb93022218866383edf7d8f6b391ef9c0b9908f9e983b08"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/109.0b2/win32/en-US/Firefox%20Setup%20109.0b2.exe#/dl.7z",
            "hash": "sha512:f2e64bfd9b06b9c821f189c503be16ae59160b26d2812e089a7c68ac837b0772fc750017ec9fd2f0d8e4f312e83fb524d957b4ad556ce59d750f6dbf60df69bc"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
