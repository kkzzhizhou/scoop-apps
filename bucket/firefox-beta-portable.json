            "hash": "sha512:6b33110d8ac175d2ebcba97fada3f76072521e5f503f45c1eebbb0dd3521d54598fb6f6b22c8a3b5fa96d074ee0668f54da3746f5ffad2810ed91f065882e95c"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/113.0b8/win32/en-US/Firefox%20Setup%20113.0b8.exe#/dl.7z",
            "hash": "sha512:24fd7ae8db333e2c6fe2ad77591e19e428986d239535e33e376dadbecbc123757b6841b69068933ea340779e22065fa8c8765384b3496375449d7d0358b688a6"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
