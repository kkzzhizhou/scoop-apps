{
    "version": "99.0b1",
    "description": "Beta builds of Firefox: the popular open source web browser.",
    "homepage": "https://www.mozilla.org/en-US/firefox/beta/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/99.0b1/win64/en-US/Firefox%20Setup%2099.0b1.exe#/dl.7z",
            "hash": "sha512:5fb5493c5988d982c55221bbd9e1a66c966d4c1d4aad919613ae821d2ca8f40c1d46a1676cc14a319ec4a7fa9dfe8f059053b296e32aa4db4bdf1dacd4bd5fc6"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/99.0b1/win32/en-US/Firefox%20Setup%2099.0b1.exe#/dl.7z",
            "hash": "sha512:5cf8ca2feaa77324d26abe045ceb2e526d6cd7524e7a18b79c92c89dff3be61720039a951814b654b8fb5a8e4f90c6358ff3f713be3844422daf26a8adcc895c"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
