            "hash": "sha512:640b6f293d537facc580df9d046e75ba7c302b64d82a5938332c4f1ebc48bb6d3c5414342ebc0f0b9f8c66cb593e36254db91dcd72b2b6377ddd5b7ee470209a"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/104.0b7/win32/en-US/Firefox%20Setup%20104.0b7.exe#/dl.7z",
            "hash": "sha512:72ab6f92749eb278b817c7b919c0c9b95e4664ccd97a0d65895d7a1291510c9e8e80b6d3f2ceb47657bc2c6e2cd71bdd17139f86db4c44984636d1908dc1325f"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
