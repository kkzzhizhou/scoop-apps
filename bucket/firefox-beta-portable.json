        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/121.0b9/win32/en-US/Firefox%20Setup%20121.0b9.exe#/dl.7z",
            "hash": "sha512:62607aa19a01066767605864438c0b08a08c6b5acb73c9b2e309909c28373f40927a6e2c5f42f4ce720a4f9b9728d80b4717fa3355cfedb5fbe48c3b6cc47c85"
