            "hash": "sha512:461c6c333713691b0199615a00ec59db0a3f15663418daf1146bf593e81b8b06c289e3983d005d105e5149cc6789e01b14a7b399e47fdc940f1fd6a57971d048"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/104.0b3/win32/en-US/Firefox%20Setup%20104.0b3.exe#/dl.7z",
            "hash": "sha512:e894db1cf3d685e64d88ecffff1291486af7b94c3212b9c9cfa183c3822583497f496c8ee3ccaeb1bdee89d5a0079a78b80dea0f176735248697209c04dfae1e"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
