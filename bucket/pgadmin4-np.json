{
    "version": "9.0",
    "homepage": "https://www.pgadmin.org/",
    "description": "PostgreSQL administration and development platform.",
    "license": "PostgreSQL",
    "architecture": {
        "64bit": {
            "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v9.0/windows/pgadmin4-9.0-x64.exe",
            "hash": "04a1a4adf93daaa810d80b8493dd4bc7dd3166f3b65d86f0e2985b80664fed87"
        }
    },
    "installer": {
        "args": [
            "/VERYSILENT",
            "/SUPPRESSMSGBOXES",
            "/SP-",
            "/NOCANCEL",
            "/NORESTART",
            "/NOICONS",
            "/DIR=$dir"
        ]
    },
    "uninstaller": {
        "file": "unins000.exe",
        "args": [
            "/VERYSILENT",
            "/SUPPRESSMSGBOXES",
            "/SP-",
            "/NOCANCEL",
            "/NORESTART"
        ]
    },
    "bin": [
        "runtime\\psql.exe",
        "runtime\\pgAdmin4.exe",
        "runtime\\pg_restore.exe",
        "runtime\\pg_dumpall.exe",
        "runtime\\pg_dump.exe"
    ],
    "shortcuts": [
        [
            "runtime\\pgAdmin4.exe",
            "pgAdmin 4"
        ]
    ],
    "checkver": {
        "url": "https://www.pgadmin.org/download/pgadmin-4-windows/",
        "regex": "pgAdmin 4 v([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v$version/windows/pgadmin4-$version-x64.exe"
            }
        }
    }
}
