{
    "version": "9.8",
    "homepage": "https://www.pgadmin.org/",
    "description": "PostgreSQL administration and development platform.",
    "license": "PostgreSQL",
    "architecture": {
        "64bit": {
            "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v9.8/windows/pgadmin4-9.8-x64.exe",
            "hash": "35b1154e2087b7c6ae28a2f95fb31816faaa2a28e769c9d2ccb48bb1928453ec"
        }
    },
    "installer": {
        "args": [
            "/VERYSILENT",
            "/SUPPRESSMSGBOXES",
            "/SP-",
            "/NOCANCEL",
            "/NORESTART",
            "/NOICONS",
            "/DIR=$dir"
        ]
    },
    "uninstaller": {
        "file": "unins000.exe",
        "args": [
            "/VERYSILENT",
            "/SUPPRESSMSGBOXES",
            "/SP-",
            "/NOCANCEL",
            "/NORESTART"
        ]
    },
    "bin": [
        "runtime\\psql.exe",
        "runtime\\pgAdmin4.exe",
        "runtime\\pg_restore.exe",
        "runtime\\pg_dumpall.exe",
        "runtime\\pg_dump.exe"
    ],
    "shortcuts": [
        [
            "runtime\\pgAdmin4.exe",
            "pgAdmin 4"
        ]
    ],
    "checkver": {
        "url": "https://www.pgadmin.org/download/pgadmin-4-windows/",
        "regex": "pgAdmin 4 v([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v$version/windows/pgadmin4-$version-x64.exe"
            }
        }
    }
}
