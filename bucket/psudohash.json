{
    "version": "master",
    "description": "Generates millions of keyword-based password mutations in seconds.",
    "homepage": "https://github.com/t3l3machus/psudohash",
    "license": "MIT",
    "suggest": "python3",
    "url": "https://github.com/t3l3machus/psudohash/archive/refs/heads/main.zip",
    "pre_install": "Set-Content \"$dir\\psudohash.bat\" '@pushd %~dp0\npython3 \"psudohash.py\" %*\n@popd' -Encoding Ascii",
    "post_install": [
        "Move-Item -Path \"$dir\\psudohash-main\\*\" -Destination \"$dir\\\"",
        "Remove-Item \"$dir\\psudohash-main\" -Force -Recurse"
    ],
    "bin": "psudohash.bat"
}
