{
    "homepage": "https://github.com/GitTools/GitVersion",
    "description": "Easy Semantic Versioning for projects using Git.",
    "license": "MIT",
    "version": "6.5.1",
    "url": "https://packages.chocolatey.org/GitVersion.Portable.6.5.1.nupkg",
    "hash": "41f528ab51b3ead6dc73cb0cc8752e8c00717fa8a1f1e0954bab3745ef6df8ef",
    "extract_dir": "tools",
    "bin": "GitVersion.exe",
    "checkver": {
        "url": "https://chocolatey.org/packages/GitVersion.Portable",
        "regex": "GitVersion ([\\w.-]+)"
    },
    "autoupdate": {
        "url": "https://packages.chocolatey.org/GitVersion.Portable.$version.nupkg",
