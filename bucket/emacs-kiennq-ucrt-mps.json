{
    "homepage": "https://github.com/kiennq/emacs-build",
    "version": "31.359.20250602.9fa0e1b",
    "description": "Scripts to build a distribution of Emacs from sources, using MSYS2 and Mingw64(32).",
    "license": "GPL (Emacs) + MIT (build script)",
    "architecture": {
        "64bit": {
            "url": "https://github.com/kiennq/emacs-build/releases/download/v31.359.20250602.9fa0e1b/emacs-9fa0e1b-ucrt-x86_64-mps.msix#/dl.7z",
            "hash": "20fb5028b166b386d81247150ecc4783f2752371b094962f2d321fb42a07d0a9"
        }
    },
    "shortcuts": [
        [
            "bin\\runemacs.exe",
            "Emacs (kiennq UCRT-mps)"
        ]
    ],
    "installer": {
        "script": [
            "Push-Location \"$dir\"",
            "dir 'bin\\*%2b*' | %{ren \"$_\" \"$($_.Name -replace '%2b','+')\"}",
            "Pop-Location"
        ]
    },
    "checkver": {
        "url": "https://api.github.com/repos/kiennq/emacs-build/releases",
        "jsonpath": "$[0].tag_name",
        "regex": "v(?<m1>[\\d.]+)\\.(?<m2>[\\d.]+)\\.(?<d1>[\\d.]+)\\.(?<h1>[\\da-z]+)",
        "replace": "${1}.${2}.${3}.${4}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/kiennq/emacs-build/releases/download/v$version/emacs-$matchH1-ucrt-x86_64-mps.msix#/dl.7z"
            }
        }
    }
}
