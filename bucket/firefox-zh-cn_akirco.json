        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/147.0.3/win32/zh-CN/Firefox%20Setup%20147.0.3.exe#/firefox.7z",
            "hash": "sha512:ccab53e4a3fd6bf7af50ebfb2307379bcea8b5026ace25e322c7ec343a42b6c4f7523e26a44a9cc0b6efb072397266e8b7f53d42c83dae686d81eb228052863e"
