            "Deemix GUI"
        ]
    ],
    "checkver": {
        "url": "https://download.deemix.app/gui/",
        "jsonpath": "$.data.files..modifiedTime",
        "regex": "([\\d]+)-([\\d]+)-([\\d]+)\\w+.|:",
        "replace": "$1.$2.$3"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.deemix.app/gui/win-x64_setup-latest.exe#dl.7z"
            }
        }
    }
