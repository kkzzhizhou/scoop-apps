        "url": "https://github.com/gchq/CyberChef/releases/download/v$version/CyberChef_v$version.zip"
    },
    "pre_install": [
        "$content = (Get-Content -Path $(Join-Path $(Find-BucketDirectory -Root -Name $bucket) starters/cyberchef.cmd)) -replace '%%HOME%%', $dir",
