{
    "homepage": "https://github.com/skylot/jadx",
    "description": "Dex to Java decompiler.",
    "version": "1.4.0",
    "license": "Apache 2.0",
    "hash": "b418619e8a2bf7ce11fd7468ce7f1170fa9a59189dedb08f31fb24d10327cbc9",
    "url": "https://github.com/skylot/jadx/releases/download/v1.4.0/jadx-1.4.0.zip",
    "suggest": {
        "JDK": [
            "java/oraclejdk",
            "java/openjdk"
        ]
    },
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/skylot/jadx/releases/download/v$version/jadx-$version.zip"
    },
    "bin": [
        "bin/jadx.bat",
        "bin/jadx-gui.bat"
    ],
    "shortcuts": [
        [
            "bin/jadx-gui.bat",
            "JADX GUI",
            "",
            "jadx-logo.ico"
        ]
    ],
    "pre_install": [
        "Add-Type -AssemblyName System.IO.Compression.FileSystem",
        "$zip = [System.IO.Compression.ZipFile]::OpenRead(\"$dir\\lib\\jadx-gui-$version.jar\")",
        "$zip.Entries | Where-Object { $_.FullName -eq \"logos/jadx-logo.ico\" } | ForEach-Object {",
        "[System.IO.Compression.ZipFileExtensions]::ExtractToFile($_, \"$dir\\jadx-logo.ico\", $true)",
        "}",
        ""
    ]
}
