{
    "version": "8.5.1",
    "description": "Fast, disk space efficient package manager -- 快速的，节省磁盘空间的包管理工具",
    "homepage": "https://pnpm.io/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/pnpm/pnpm/releases/download/v8.5.1/pnpm-win-x64.exe#/pnpm.exe",
            "hash": "bb86ced4e0e0e3441e2d0642b9ee2d9007000e2f5750f10545479c6bfe86d7c5"
        }
    },
    "post_install": [
        "pnpm config set store-dir $dir\\store",
        "pnpm config set global-dir $dir\\global",
        "pnpm config set global-bin-dir $dir\\bin",
        "pnpm config set cache-dir $dir\\cache",
        "pnpm config set state-dir $dir\\state"
    ],
    "bin": "pnpm.exe",
    "persist": [
        "store",
        "global",
        "bin",
        "cache",
        "state"
    ],
    "env_add_path": "bin",
    "checkver": {
        "github": "https://github.com/pnpm/pnpm"
    },
    "autoupdate": {
        "url": "https://github.com/pnpm/pnpm/releases/download/v$version/pnpm-win-x64.exe#/pnpm.exe",
        "hash": {
            "mode": "download"
        }
    }
}
