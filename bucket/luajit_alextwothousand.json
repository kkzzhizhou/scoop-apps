{
    "version": "2.1.0-beta3",
    "notes": [
        "LuaJIT is a Just-In-Time Compiler (JIT) for the Lua programming language.",
        "Scoop bucket by Alex <alextwothousand/scoop-bucket>"
    ],
    "url": "https://luajit.org/download/LuaJIT-2.1.0-beta3.zip",
    "hash": "fcc4069bfaf909f762844d6404a8c5940591b00237ffce1781e887a0964140da",
    "extract_dir": "LuaJIT-2.1.0-beta3",
    "depends": {
        "make": "main/make",
        "curl": "main/curl"
    },
    "pre_install": [
        "cd $dir\\src && make",
        "cd $dir\\src && curl https://gist.githubusercontent.com/alextwothousand/7f2f1ac87e680b85f09cf36664538051/raw/3f9faad0b7d4d2596a7e07000c6cf3ff370fa57c/lua.cmd --output lua.cmd"
    ],
    "bin": "src\\luajit.exe",
    "persist": "data",
    "env_add_path": "src",
    "env_set": {
        "LUAJIT_PATH": "$dir\\src"
    }
}
