{
    "version": "1.3.1",
    "description": "A PC port and coop mod of Super Mario 64",
    "homepage": "https://sm64coopdx.com/",
    "license": "Unlicense",
    "notes": [
        "ATTENTION: sm64coopdx requires specific n64 rom(s) of Super Mario 64 to function.",
        "Place the rom(s) in $persist_dir."
    ],
    "architecture": {
        "64bit": {
            "url": "https://github.com/coop-deluxe/sm64coopdx/releases/download/v1.3.1/sm64coopdx_v1.3.1_Windows_OpenGL.zip",
            "hash": "aeae13b4741440b86daf9f1aec750af8863f6e033359a1d7c7bbaceefce30d8c"
        }
    },
    "bin": "sm64coopdx.exe",
    "shortcuts": [
        [
            "sm64coopdx.exe",
            "sm64coopdx [win][sm64]"
        ]
    ],
    "persist": [
        "mods",
        "dynos",
        "palettes"
    ],
    "checkver": {
        "github": "https://github.com/coop-deluxe/sm64coopdx/",
        "regex": "v(?<version>([\\d\\.]+))",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/coop-deluxe/sm64coopdx/releases/download/v$version/sm64coopdx_v$version_Windows_OpenGL.zip"
            }
        }
    }
}
