{
    "version": "1.4",
    "description": "A PC port and coop mod of Super Mario 64",
    "homepage": "https://sm64coopdx.com/",
    "license": "Unlicense",
    "notes": [
        "ATTENTION: sm64coopdx requires specific n64 rom(s) of Super Mario 64 to function.",
        "Place the rom(s) in $persist_dir."
    ],
    "architecture": {
        "64bit": {
            "url": "https://github.com/coop-deluxe/sm64coopdx/releases/download/v1.4/sm64coopdx_Windows_OpenGL.zip",
            "hash": "338fd902d1b5b1307f7e5877576033a3ad28dac42c53939a79455549ffba050f"
        }
    },
    "bin": "sm64coopdx.exe",
    "shortcuts": [
        [
            "sm64coopdx.exe",
            "sm64coopdx [win][sm64]"
        ]
    ],
    "persist": [
        "mods",
        "dynos",
        "palettes"
    ],
    "checkver": {
        "github": "https://github.com/coop-deluxe/sm64coopdx/",
        "regex": "v(?<version>[0-9]+\\.[0-9]+(?:\\.[0-9]+)?)",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/coop-deluxe/sm64coopdx/releases/download/v$version/sm64coopdx_Windows_OpenGL.zip"
            }
        }
    }
}
