{
    "version": "3.9.6",
    "description": "CUDA on AMD GPUs.",
    "homepage": "https://github.com/lshqqytiger/ZLUDA",
    "license": {
        "identifier": "Apache-2.0",
        "url": "https://github.com/lshqqytiger/ZLUDA/blob/master/LICENSE"
    },
    "url": "https://github.com/lshqqytiger/ZLUDA/releases/download/rel.854c58e1565c3597e17046d7cc2b1eab96fcdf55/ZLUDA-windows-rocm5-amd64.zip",
    "hash": "aa236590637e4751f97bcfc1fe7c67b70d259cdb958fdfd88603e48c2ccd566f",
    "bin": "zluda.exe",
    "env_add_path": ".",
    "checkver": {
        "script": [
            "$rel_list = Invoke-RestMethod -Uri 'https://api.github.com/repos/lshqqytiger/ZLUDA/releases'",
            "$rel = $rel_list | Sort-Object {$_.published_at} | Select-Object -last 1",
            "$assets = $rel.assets | Where-Object { $_.name -match 'windows' } | Sort-Object -Descending {if ($_.name -match '-rocm(\\d+)-') {([int]$Matches[1] + [int]($_.name -match '-nightly-'))} else {0}}",
            "$download_asset = $assets | Where-Object { $_.name -match 'windows' } | Select-Object -first 1",
            "Write-Output $rel.name $download_asset.browser_download_url $download_asset.digest"
        ],
        "regex": "(?<version>[\\d.]+)\\s(?<url>[^\\s]+)\\s(?<hash>[^\\s]+)?"
    },
    "installer": {
        "script": [
            "if (Test-Path \"$dir\\zluda\") {",
            "Move-Item \"$dir\\zluda\\*\" \"$dir\"",
            "Remove-Item -Recurse \"$dir\\zluda\"",
            "}"
        ]
    },
    "autoupdate": {
        "url": "$matchUrl",
        "hash": "$matchHash"
    }
}
