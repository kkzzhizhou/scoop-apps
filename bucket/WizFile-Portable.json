{
    "version": "3.10",
    "description": "A file search utility that search for files on your hard drive by name.",
    "homepage": "https://antibody-software.com/wizfile/",
    "license": "Unknown",
    "changelog": "https://antibody-software.com/wizfile/whats-new",
