        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://github.com/Jigsaw-Code/outline-server/releases/download/v$version/Outline-Manager.exe#/dl.zip",
        "hash": {
            "url": "https://github.com/Jigsaw-Code/outline-server/releases/download/v$version/latest.yml",
            "regex": "sha512: $base64"
