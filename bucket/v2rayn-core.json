{
    "version": "5.32",
    "description": "GUI client based for v2ray-core and xray-core.",
    "homepage": "https://github.com/2dust/v2rayN",
    "license": "GPL-3.0-only",
    "url": "https://github.com/2dust/v2rayN/releases/download/5.32/v2rayN-Core.zip",
    "hash": "5606ce89cfb1ecc0bb988f5023aed652f2659995219b2eb42f590443047b7031",
    "extract_dir": "v2rayN-Core",
    "pre_install": [
        "foreach ($name in @('guiNConfig.json', 'user-wininet.json')) {",
        "    if (!(Test-Path \"$persist_dir\\$name\")) {",
        "        New-Item -Force -Path \"$dir\" -Name $name | Out-Null",
        "    }",
        "}"
    ],
    "bin": "v2rayN.exe",
    "shortcuts": [
        [
            "v2rayN.exe",
            "v2rayN"
        ]
    ],
    "persist": [
        "user-wininet.json",
        "config.json",
        "guiNConfig.json"
    ],
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/2dust/v2rayN/releases/download/$version/v2rayN-Core.zip"
    }
}
