    "version": "2023.03.21",
    "url": "https://github.com/eth-p/bat-extras/releases/download/v2023.03.21/bat-extras-20230321.zip",
    "hash": "c2469767c7e76f9d9c4a25574a6ed895754e05410f4ba34f534c148e09dca725",
    "installer": {
        "script": [
            "'batgrep', 'batman' , 'batpipe' , 'batwatch' , 'batdiff' , 'prettybat' | ForEach-Object {",
            "Set-Content \"$dir\\$_.bat\" \"@for /f %%i in ('scoop prefix git') do @%%i\\bin\\bash.exe %~dp0bin\\%~n0 %*\" -Encoding ASCII",
            "}"
        ]
    },
    "bin": [
        "batgrep.bat",
        "batman.bat",
        "batpipe.bat",
        "batwatch.bat",
        "batdiff.bat",
        "prettybat.bat"
    ],
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/eth-p/bat-extras/releases/download/v$version/bat-extras-$cleanVersion.zip"
    }
}
