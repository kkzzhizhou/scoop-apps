    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://packages.edgedb.com/archive/x86_64-pc-windows-msvc/edgedb-cli-$version+$matchCommit.exe#/edgedb.exe"
            }
        }
    }
