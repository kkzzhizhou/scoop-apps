    "bin": "edgedb.exe",
    "checkver": {
        "script": [
            "$indexes = Invoke-RestMethod 'https://packages.edgedb.com/archive/.jsonindexes/x86_64-pc-windows-msvc.json'",
            "$latest = $indexes.packages | Sort-Object -Property revision | Select-Object -Last 1",
            "return $latest.version"
        ],
