    "hash": "e751276e31b47f51f81a8d645d56ff53991f9a84103c93904df40ee1d816f348",
    "architecture": {
        "32bit": {
            "pre_install": "Rename-Item \"$dir/LinpackXtreme_x32.exe\" LinpackXtreme.exe"
        },
        "64bit": {
            "pre_install": "Rename-Item \"$dir/LinpackXtreme_x64.exe\" LinpackXtreme.exe"
        }
    },
    "installer": {
        "script": [
            "@\"",
            "cd \"$dir\"",
            "LinpackXtreme.exe",
            "\"@ | Set-Content \"$dir/LinpackXtreme.bat\"",
            "$null = 'results.txt', 'linpack.ini' | ForEach-Object { $_ = \"/$_\"; if (!(Test-Path \"$persist_dir$_\")) { New-Item \"$dir$_\" } }"
        ]
    },
    "bin": "LinpackXtreme.bat",
    "persist": [
        "results.txt",
        "linpack.ini",
        "hwmonitor/hwmonitorw.ini"
    ],
    "checkver": {
        "regex": "Changes \\(v([^)]+)"
    },
    "autoupdate": {
        "url": "https://sea-02.dlo4d.com/files/linpack-xtreme/LinpackXtreme.zip"
    }
