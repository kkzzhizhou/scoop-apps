    ],
    "bin": "jpegqs.exe",
    "shortcuts": [
        [
            "jpegqs_gui.exe",
            "JPEG Quant Smooth GUI"
        ]
    ],
    "checkver": {
        "script": [
            ":releases foreach ($release in (Invoke-RestMethod https://api.github.com/repositories/232038116/releases)) {",
            "   foreach ($asset in $release.assets) {",
            "        if ($asset.name -ceq 'jpegqs_windows.zip') {",
            "            $release.tag_name",
            "            break releases",
            "        }",
            "    }",
            "}"
        ],
        "regex": "\\A([\\d.]+)\\Z"
    },
    "autoupdate": {
        "url": "https://github.com/ilyakurdyukov/jpeg-quantsmooth/releases/download/$version/jpegqs_windows.zip"
    }
