{
    "version": "4.9",
    "description": "cobalt strike简称CS，是一款GUI框架式的渗透测试工具.(Scoop bucket by arch3rpro)",
    "homepage": "https://www.cobaltstrike.com/",
    "license": "Proprietary",
    "notes": [
        "启动teamserver需要安装WSL并启动wslpp自动设置端口映射",
        "自行安装时注意修改实际启动的目录"
    ],
    "url": "http://pan.assass1n.top/directlink/Share/CobaltStrike4.9.zip",
    "hash": "E268411FDBBDB5577D1296E75704F53FABCA1E970E479580C7598748FE657051",
    "pre_install": [
        "$teamserver = {",
        "    $argString = $args -join ' '",
        "    wsl -u root -- bash -c \"cd /mnt/d/Global/apps/cobaltstrike/current/Server/ && ./teamserver $argString\"",
        "}",
        "Set-Content \"$dir\\teamserver.ps1\" $teamserver -Encoding Ascii",
        "Set-Content \"$dir\\CobaltStrike.bat\" '@echo off\npushd %~dp0\\Client\nstart cmd.exe /K \"cobaltstrike-client.cmd\"\npopd' -Encoding Ascii"
    ],
    "bin": [
        [
            "CobaltStrike.bat",
            "cobaltstrike"
        ],
        [
            "teamserver.ps1",
            "teamserver"
        ]
    ]
}
