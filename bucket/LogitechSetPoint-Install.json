    "description": "SetPoint lets you customize your mouse, keyboard, touchpad and number pad settings in Windows",
    "homepage": "http://support.logitech.com/en_gb/software/setpoint",
    "license": "",
    "architecture": {
        "64bit": {
            "url": "https://download01.logi.com/web/ftp/pub/techsupport/mouse/SetPoint6.69.126_64.exe#/setup.exe",
            "hash": "bb0f093dd6cdd67b90e6a200cb4be49b1011b98ba62ab3aedc521a85526e157a"
        },
        "32bit": {
            "url": "https://download01.logi.com/web/ftp/pub/techsupport/mouse/SetPoint6.69.126_32.exe#/setup.exe",
            "hash": "a0b9e2477f0bdc32f69e8695a87115c5b5d58789947f89c0a34d28bba624cda9"
        }
    },
    "pre_install": [
        "$processname = \"SetPoint\"",
        "$process = Get-Process $processname -ErrorAction SilentlyContinue",
        "if ($process) {",
        "   $process.CloseMainWindow()",
        "   Start-Sleep -Seconds 5",
        "   if (!$process.HasExited) {",
        "       Start-Sleep -Seconds 25",
        "       $process | Stop-Process -Force",
        "   }",
        "}"
    ],
    "installer": {
        "script": [
            "$programname = \"Logitech SetPoint*\"",
            "if(!(Get-Package -Provider Programs -IncludeWindowsInstaller -Name \"$programname\" -MinimumVersion $version -ErrorAction SilentlyContinue))",
            "{",
            "   &\"$dir\\setup.exe\" /S | Out-Null",
            "   if(!(Get-Package -Provider Programs -IncludeWindowsInstaller -Name \"$programname\" -MinimumVersion $version -ErrorAction SilentlyContinue))",
            "   {",
            "       exit 1",
            "   }",
            "}",
            "Remove-Item \"$dir\\setup.exe\" -Force"
        ]
    },
    "checkver": {
        "url": "https://chocolatey.org/packages/setpoint",
        "regex": "Downloads of v ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download01.logi.com/web/ftp/pub/techsupport/mouse/SetPoint$version_64.exe#/setup.exe"
            },
            "32bit": {
                "url": "https://download01.logi.com/web/ftp/pub/techsupport/mouse/SetPoint$version_32.exe#/setup.exe"
            }
        }
    }
}
