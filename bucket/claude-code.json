{
    "version": "2.1.3",
    "description": "An agentic coding tool that lives in your terminal (with bun)",
    "homepage": "https://claude.com/product/claude-code",
    "license": "Proprietary",
    "url": "http://www.gstatic.com/generate_204#/dummy",
    "hash": "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855",
    "bin": [
        [
            "node_modules\\.bin\\claude.exe",
            "claude"
        ]
    ],
    "installer": {
        "script": "pushd $dir; bun add --bun @anthropic-ai/claude-code@2.1.3; popd;"
    },
    "uninstaller": {
        "script": "pushd $dir; bun rm @anthropic-ai/claude-code; popd;"
    },
    "depends": [
        "bun",
        "git"
    ],
    "env_set": {
        "CLAUDE_CODE_GIT_BASH_PATH": "$scoopdir\\apps\\git\\current\\bin\\bash.exe"
    },
    "checkver": {
        "url": "https://github.com/anthropics/claude-code/tags",
        "regex": "/v([\\d.]+)\"(?!-)"
    },
    "autoupdate": {
        "installer": {
            "script": "pushd $dir; bun add --bun @anthropic-ai/claude-code@$version; popd;"
        }
    }
}
