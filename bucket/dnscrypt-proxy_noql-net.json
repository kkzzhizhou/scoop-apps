            }
        }
    },
    "pre_install": [
        "'dnscrypt-proxy.toml', 'query.log' | ForEach-Object {",
        "    if (!(Test-Path \"$persist_dir\\$_\")) { New-Item \"$dir\\$_\" -ItemType File | Out-Null }",
        "};"
    ],
    "persist": [
        "dnscrypt-proxy.toml",
        "query.log"
    ],
    "extract_dir": "win64",
    "bin": "dnscrypt-proxy.exe"
}
