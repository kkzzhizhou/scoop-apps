    "suggest": {
        "ffmpeg": [
            "ffmpeg",
            "ffmpeg-nightly"
        ],
        "nssm": "nssm"
    },
    "architecture": {
        "64bit": {
            "url": "https://repo.jellyfin.org/files/server/windows/latest-stable/amd64/jellyfin_10.10.7-amd64.zip",
            "hash": "29d4d2a837152e7f644ed8f2b2b456fdf5c422f3d7fb2cc7996c36ab7b631d48"
        }
    },
    "extract_dir": "jellyfin_10.10.7",
