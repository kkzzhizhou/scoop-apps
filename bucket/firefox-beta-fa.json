    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/127.0b9/win64/fa-IR/Firefox%20Setup%20127.0b9.exe#/dl.7z",
            "hash": "sha512:4cae90f5c107f1f01e11a55ca3d11f7eb04c4c97dc26810804c30818b4f6cc1a34fb60184d40803464818504369cb65724a3124838a794238eb2db5f1b2c7653"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/127.0b9/win32/fa-IR/Firefox%20Setup%20127.0b9.exe#/dl.7z",
            "hash": "sha512:4cae90f5c107f1f01e11a55ca3d11f7eb04c4c97dc26810804c30818b4f6cc1a34fb60184d40803464818504369cb65724a3124838a794238eb2db5f1b2c7653"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta-fa",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta (fa)",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/fa-IR/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/fa-IR/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/fa-IR/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/fa-IR/$basename"
                }
            }
        }
    }
}
