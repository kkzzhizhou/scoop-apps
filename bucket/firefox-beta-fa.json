    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta (fa)",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
